{
  "name": "tienda-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:prod": "npm run clean:win && cross-env NODE_ENV=production NEXT_PUBLIC_BUILD_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next out",
    "clean:win": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out",
    "serve": "npx serve out -p 3000",
    "check-build:win": "if exist out (dir out /s)",
    "deploy:prod": "npm run build:prod && npm run check-build:win",
    "prebuild:prod": "echo Building tienda-front for production..."
  },
  "dependencies": {
    "@heroui/button": "^2.2.19",
    "@heroui/input": "^2.4.19",
    "@heroui/system": "^2.4.15",
    "@heroui/theme": "^2.4.15",
    "@tailwindcss/forms": "^0.4.0",
    "axios": "^1.9.0",
    "framer-motion": "^12.9.7",
    "install": "^0.13.0",
    "lucide-react": "^0.546.0",
    "next": "^15.3.4",
    "npm": "^11.4.2",
    "primereact": "^10.9.5",
    "react": "^18.3.1",
    "react-countup": "^6.1.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.3.1",
    "react-responsive-modal": "^6.4.2",
    "recharts": "^2.15.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-config-next": "^13.0.5",
    "postcss": "^8.4.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.0.5"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=8.x"
  }
}
